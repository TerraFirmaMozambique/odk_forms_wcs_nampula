instance id="assoc_names_wcs_nampula" src="jr://file-csv/assoc_names_wcs_nampula.csv"/><instance id="parties_wcs_nampula" src="jr://file-csv/parties_wcs_nampula.csv"/><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/><bind calculate="&quot;fixed&quot;" nodeset="/data/external_id" type="string"/><bind calculate="&quot;WCS - Futura Azul&quot;" nodeset="/data/project" type="string"/><bind calculate="&quot;Formulário P - Parcelas&quot;" nodeset="/data/my_form_name" type="string"/><bind calculate=" /data/deviceid " nodeset="/data/user_id" type="string"/><bind calculate="&quot;Non Biometric&quot;" nodeset="/data/project_id" type="string"/><bind nodeset="/data/g1/logo" readonly="true()" type="string"/><bind nodeset="/data/g1/cavateco" readonly="true()" type="string"/><bind nodeset="/data/intronote" readonly="true()" type="string"/><bind nodeset="/data/form_o_confirmed" required="true()"/><bind nodeset="/data/searchtext" required="true()" type="string"/><bind nodeset="/data/enum_name_id" required="true()" type="string"/><bind calculate="pulldata('enum_names_wcs_nampula', 'enumerator_name', 'name_id_key',  /data/enum_name_id )" nodeset="/data/enum_name" type="string"/><bind constraint=". &lt;= today()" nodeset="/data/date" required="true()" type="date"/><bind nodeset="/data/prov_id" required="true()" type="string"/><bind nodeset="/data/dist_id" required="true()" type="string"/><bind nodeset="/data/post_id" required="true()" type="string"/><bind nodeset="/data/assoc_id" required="true()" type="string"/><bind calculate="pulldata('assoc_names_wcs_nampula', 'assoc_name', 'assoc_id_key',  /data/assoc_id )" nodeset="/data/assoc_name" type="string"/><bind calculate=" /data/assoc_id " nodeset="/data/module_id" type="string"/><bind constraint="regex(.,'[0-9]{6}')" nodeset="/data/receipt_id" required="true()" type="string"/><bind constraint=".= /data/receipt_id " jr:constraintMsg="O número do recibo que reentrou não corresponde à sua primeira entrada. Faça a favor de verificar de novo." nodeset="/data/receipt_id_check" required="true()" type="string"/><bind nodeset="/data/occup_type" required="true()" type="string"/><bind nodeset="/data/norm_type" relevant=" /data/occup_type ='customary'" required="true()" type="string"/><bind nodeset="/data/zone_exclusion_y_n" required="true()" type="string"/><bind nodeset="/data/g3" relevant=" /data/zone_exclusion_y_n = 'yes'"/><bind nodeset="/data/g3/zone_overlap" required="true()" type="string"/><bind nodeset="/data/g3/zone_type" required="true()" type="string"/><bind nodeset="/data/conflict_y_n" required="true()" type="string"/><bind nodeset="/data/g4" relevant=" /data/conflict_y_n = 'yes'"/><bind nodeset="/data/g4/conflict_type" required="true()" type="string"/><bind constraint="regex(., &quot;^[a-zA-Z0-9. _-éáíóãõúçâôàèê]{1,255}$&quot;)" jr:constraintMsg="Parágrafos não são permitidos. Por favor, eliminar quaisquer linhas extras." nodeset="/data/g4/conflict_description" required="true()" type="string"/><bind nodeset="/data/g4/solved_y_n" required="true()" type="string"/><bind nodeset="/data/g5/use_category" required="true()" type="string"/><bind nodeset="/data/g5/use_prin" required="true()" type="string"/><bind constraint=".&gt;1" jr:constraintMsg="Tem que ser pelo menos 2 pessoas!" nodeset="/data/parties_number" required="true()" type="int"/><bind calculate=" /data/parties_number " nodeset="/data/parties_count" readonly="true()" type="string"/><bind nodeset="/data/parties/party_role" required="true()" type="string"/><bind nodeset="/data/parties/searchtext_1" required="true()" type="string"/><bind nodeset="/data/parties/party_id" required="true()" type="string"/><bind calculate="pulldata('parties_wcs_nampula', 'party_name', 'party_id_key',  ../party_id )" nodeset="/data/parties/party_name" type="string"/><bind calculate="pulldata('parties_wcs_nampula', 'party_id_key', 'party_id_key',  ../party_id )" nodeset="/data/parties/selected_user_guid" type="string"/><bind calculate="pulldata('parties_wcs_nampula', 'id_document', 'party_id_key',  ../party_id )" nodeset="/data/parties/party_id_document" type="string"/><bind calculate="pulldata('parties_wcs_nampula', 'id_number', 'party_id_key',  ../party_id )" nodeset="/data/parties/party_id_number" type="string"/><bind nodeset="/data/parties/id_confirm" required="true()"/><bind nodeset="/data/parties/rep_relation" relevant=" ../party_role = 'representative_minor' or  ../party_role = 'representative_absent'" required="true()" type="string"/><bind calculate="if( ../party_role  = 'witness', 1, 0 )" nodeset="/data/parties/witness_num" type="string"/><bind calculate="if( ../party_role  = 'right_holder', 1, 0 )" nodeset="/data/parties/title_num" type="string"/><bind calculate="sum( /data/parties/witness_num )" nodeset="/data/witness_count" type="string"/><bind calculate="sum( /data/parties/title_num )" nodeset="/data/title_count" type="string"/><bind constraint=".&gt;0" jr:constraintMsg="Não foi selecionada nenhuma testemunha e não pode continuar." nodeset="/data/no_witness_block" relevant=" /data/witness_count =0" required="true()"/><bind constraint=".&gt;0" jr:constraintMsg="Não foi selecionada nenhum titular e não pode continuar." nodeset="/data/no_title_block" relevant=" /data/title_count =0" required="true()"/><bind nodeset="/data/auto_gps" required="true()" type="geopoint"/><bind nodeset="/data/limits_y_n" required="true()" type="string"/><bind nodeset="/data/Pontos" relevant=" /data/limits_y_n = 'yes'"/><bind nodeset="/data/Pontos/auto_gps_limit" required="true()" type="geopoint"/><bind calculate="concat( /data/receipt_id ,'point')" nodeset="/data/Pontos/upn_gps_limit" type="string"/><bind nodeset="/data/endnote" readonly="true()" type="string"/><bind nodeset="/data/receipt_image" required="true()" type="binary"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/><bind calculate="concat('parcelas','_', /data/assoc_name ,'_', /data/receipt_id )" nodeset="/data/meta/instanceName" type="string"/></model></h:head><h:body><group appearance="field-list" ref="/data/g1"><input ref="/data/g1/logo"><label ref="jr:itext('/data/g1/logo:label')"/></input><input ref="/data/g1/cavateco"><label>&lt;br/&gt; &lt;h1&gt;Cadeia de Valor de Terra Comunitária&lt;/h1&gt;&lt;br/&gt; &lt;h2&gt;<output value=" /data/project "/>&lt;/h2&gt;&lt;span style=&quot;color:#57b055&quot;&gt;<output value=" /data/my_form_name "/>&lt;/span&gt; &lt;br/&gt;</label></input></group><input ref="/data/intronote"><label>&lt;h1&gt;&lt;span style=&quot;color:#FF0000&quot;&gt;<output value=" /data/project "/>&lt;/span&gt;&lt;br/&gt; &lt;h1&gt;&lt;span style=&quot;color:#57b055&quot;&gt;<output value=" /data/my_form_name "/>&lt;/span&gt; &lt;br/&gt;

Este formulário é utilizado para identificar as parcelas que serão certificadas pela associação. 

O técnico pode usar o formulário para fazer registo da parcela no campo: a forma de obtenção, o uso, os limites e o(s) titular(es) e testemunha(s).

&lt;span style=&quot;color:#FF6347&quot;&gt; ANTES DE PREENCHER este Formulário&lt;/span&gt;, é necessário verificar que o(s) titular(es) e testemunha(s) já fizeram o registo do seus dados através do 'Formulário O - Pessoas'. Assim, o(s) nome(s) do(s) titular(es) e testemunha(s) podem ser selecionados duma lista. 

Se as pessoas (titulares ou testemunhas) não fizeram o registo dos nomes durante a fase de pre-registo, não será possível fazer o registo da parcela. Estas pessoas devem ser adicionadas primeiro, utilizando o 'Formulário O - Pessoas'. Depois de fazer a actualização dos dados, que normalmente vai acontecer até o dia seguinte de submissão, será possível visualizar o nome da pessoa e fazer o registo da parcela.

É permitida a demarcação/identificação das parcelas por pessoas que actuam como representantes dos proprietários, quando os proprietários estão ausentes, contudo, os proprietários têm de ter sido previamente registados através do 'Formulário O - Pessoas'. 

O formulário é usado junto com os mapas para identificar os limites de parcela. Os limites devem ser indicados no mapa, e o formulário também permite a gravação das coordenadas geográficas dos limites, quando necessário. 

No fim do formulário, o técnico deve preencher um recibo, tirar uma foto do recibo, e entrega-lo ao titular. 

Por favor deslizar para a frente para continuar.</label></input><trigger ref="/data/form_o_confirmed"><label>Confirmação de Registo de Pessoas</label><hint>Confirmo que todos os titulares e testemunhas já registraram seus nomes e detalhes pessoais</hint></trigger><input ref="/data/searchtext"><label>Digite a primeira letra do seu nome para filtrar a lista de enumeradores já registadas:</label></input><select1 appearance="search('enum_names_wcs_nampula', 'startswith', 'enumerator_name',  /data/searchtext )" ref="/data/enum_name_id"><label>Selecionar o seu nome:</label><hint>&lt;h3&gt;Se o seu nome &lt;span style="color:#FF6347"&gt;não aparece&lt;/span&gt; em baixo, quer dizer que ainda não fez o registo dos seus dados, ou que houve problema na actualização do sistema. Não será possível continuar. Deve fazer o seu registo de pessoa através do 'Formulário 1', e depois actualizar os dados, e voltar a repetir o registo de ponto mais tarde. Pode fechar o Formulário sem gravar.</hint><item><label>enumerator_name</label><value>name_id_key</value></item></select1><input appearance="no-calendar" ref="/data/date"><label>Data do registo de parcela:</label></input><select1 appearance="minimal" ref="/data/prov_id"><label>Selecionar a província:</label><item><label>Cabo Delgado</label><value>P02</value></item><item><label>Gaza</label><value>P09</value></item><item><label>Inhambane</label><value>P08</value></item><item><label>Manica</label><value>P06</value></item><item><label>Maputo</label><value>P10</value></item><item><label>Nampula</label><value>P03</value></item><item><label>Niassa</label><value>P01</value></item><item><label>Sofala</label><value>P07</value></item><item><label>Tete</label><value>P05</value></item><item><label>Zambezia</label><value>P04</value></item></select1><input appearance="minimal" ref="/data/dist_id" query="instance('districts')/root/item[prov_id= /data/prov_id ]"><label>Selecionar o distrito:</label></input><input appearance="minimal" ref="/data/post_id" query="instance('posts')/root/item[dist_id= /data/dist_id ]"><label>Selecionar o posto administrativo:</label></input><select1 appearance="search('assoc_names_wcs_nampula', 'matches', 'post_id',  /data/post_id )" ref="/data/assoc_id"><label>Selecionar a associação</label><hint>Se o nome da associação da comunidade não aparecer na lista abaixo, pode ser que não houve ainda o registo prévio da associação ou que você não fez a actualização do formulário. A associação deve ser registada através do 'Formulário F - Registo de Associação', e os dados devem ser submetidos e actualizados. 

&lt;span style="color:#FF0000"&gt;Deve fechar o formulário sem gravando as alterações e informar o supervisor&lt;/span&gt;</hint><item><label>assoc_name</label><value>assoc_id_key</value></item></select1><input appearance="numbers" ref="/data/receipt_id"><label>Qual é o numero do recibo?</label></input><input appearance="numbers" ref="/data/receipt_id_check"><label>Por favor, re-digite o número do recibo como confirmação:</label></input><input appearance="minimal" ref="/data/occup_type" query="instance('occup')/root/item[external_id= /data/external_id ]"><label>Qual é a origem do DUAT?</label></input><input appearance="minimal" ref="/data/norm_type" query="instance('norms')/root/item[external_id= /data/external_id ]"><label>Sendo através de normas e práticas costumeiras, qual foi a forma de obtenção da parcela?</label></input><input appearance="minimal" ref="/data/zone_exclusion_y_n" query="instance('y_n')/root/item[external_id= /data/external_id ]"><label>A parcela está localizada numa zona de exclusão?</label></input><group appearance="field-list" ref="/data/g3"><label>Detalhes de zona</label><input appearance="minimal" ref="/data/g3/zone_overlap" query="instance('zone_overlaps')/root/item[external_id= /data/external_id ]"><label>É apenas uma parte da parcela dentro da zona de exclusão ou toda a parcela?</label></input><input appearance="minimal" ref="/data/g3/zone_type" query="instance('zone_types')/root/item[external_id= /data/external_id ]"><label>Que tipo de zona de exclusão?</label></input></group><input appearance="minimal" ref="/data/conflict_y_n" query="instance('y_n')/root/item[external_id= /data/external_id ]"><label>Será que houve qualquer conflito ou disputa sobre esta parcela, agora ou no passado?</label></input><group appearance="field-list" ref="/data/g4"><label>Detalhes de conflito ou disputa</label><input appearance="minimal" ref="/data/g4/conflict_type" query="instance('conflict_types')/root/item[external_id= /data/external_id ]"><label>Que tipo de conflito ou disputa?</label></input><input ref="/data/g4/conflict_description"><label>Breve descrição de conflito ou disputa:</label></input><input appearance="minimal" ref="/data/g4/solved_y_n" query="instance('y_n')/root/item[external_id= /data/external_id ]"><label>O conflito ou disputa sobre a parcela ficou resolvido através deste projecto?</label></input></group><group appearance="field-list" ref="/data/g5"><label>Detalhes de uso</label><input appearance="minimal" ref="/data/g5/use_category" query="instance('use_categories')/root/item[external_id= /data/external_id ]"><label>Forma de uso:</label></input><input appearance="minimal" ref="/data/g5/use_prin" query="instance('uses_parcel')/root/item[external_id= /data/external_id ]"><label>Qual é o uso principal?</label></input></group><input ref="/data/parties_number"><label>Quantas pessoas estão ligadas à parcela?

&lt;span style="color:#FF6347"&gt;Tem que ser pelo menos 2 em total, e incluir pelo menos 1 titular e pelo menos 1 testemunha&lt;/span&gt;</label></input><group ref="/data/parties"><label>Pessoas</label><repeat jr:count=" /data/parties_count " nodeset="/data/parties"><input ref="/data/parties/party_role" query="instance('party_role')/root/item[external_id= /data/external_id ]"><label>Esta pessoa é o titular, representante duma pessoa menor, representante duma pessoa ausente ou testemunha?</label></input><input ref="/data/parties/searchtext_1"><label>Digite as primeiras letras do nome a ser pesquisada da lista de pessoas já registadas:</label></input><select1 appearance="search('parties_wcs_nampula', 'startswith', 'party_name',  ../searchtext_1 ,'assoc_id',  /data/assoc_id )" ref="/data/parties/party_id"><label>Selecionar o nome da pessoa:</label><hint>&lt;h3&gt;Se o nome do titular ou testemunha &lt;span style="color:#FF6347"&gt;não aparece&lt;/span&gt; em baixo, quer dizer que a pessoa ainda não fez o registo dos seus dados, ou que houve problema na actualização do sistema. Não será possível continuar. Deve fazer o registo de pessoa através do 'Formulário O - Pessoas', e depois actualizar os dados, e voltar a repetir o registo de parcela mais tarde. Pode fechar o Formulário sem gravar.</hint><item><label>party_name</label><value>party_id_key</value></item></select1><trigger ref="/data/parties/id_confirm"><label>Confirmação de Identidade de Pessoa</label><hint>&lt;h1&gt;&lt;span style=&quot;color:#042f66&quot;&gt;Detalhes da pessoa:&lt;/span&gt;

&lt;h2&gt;Nome : &lt;span style=&quot;color:#042f66&quot;&gt;<output value=" ../party_name "/>&lt;/span&gt;

&lt;h2&gt;Tipo de identidade : &lt;span style=&quot;color:#042f66&quot;&gt;<output value=" ../party_id_document "/>&lt;/span&gt;

&lt;h2&gt;Numero de ID : &lt;span style=&quot;color:#042f66&quot;&gt;<output value=" ../party_id_number "/>&lt;/span&gt;


&lt;h1&gt;&lt;span style=&quot;color:#57b055&quot;&gt;Através destes detalhes, eu (enumerador), e as outras pessoas presentes, testemunhamos que a identidade desta pessoa fica confirmada.&lt;/span&gt;</hint></trigger><input ref="/data/parties/rep_relation" query="instance('rep_relations')/root/item[external_id= /data/external_id ]"><label>Relação com o titular:</label></input></repeat></group><trigger ref="/data/no_witness_block"><label>&lt;h1&gt;&lt;span style="color:#FF6347"&gt;Não selecionou nehuma testemunha!&lt;/span&gt; 

&lt;h1&gt;Tem que voltar e 

&lt;h1&gt;Mudar a categoria duma pessoa já registada 

&lt;h1&gt;ou 

&lt;h1&gt;Adicionar mais uma pessoa como testamunha</label></trigger><trigger ref="/data/no_title_block"><label>&lt;h1&gt;&lt;span style="color:#FF6347"&gt;Não selecionou nehum titular!&lt;/span&gt; 

&lt;h1&gt;Tem que voltar e 

&lt;h1&gt;Mudar a categoria duma pessoa já registada 

&lt;h1&gt;ou 

&lt;h1&gt;Adicionar mais uma pessoa como titular</label></trigger><input accuracyThreshold="1.5" ref="/data/auto_gps"><label>É obrigatório gravar um ponto dentro da parcela, no centro.</label></input><input ref="/data/limits_y_n" query="instance('y_n')/root/item[external_id= /data/external_id ]"><label>Será que é necessário gravar pontos de limites da parcela (porque não estão claros no mapa)?</label></input><group ref="/data/Pontos"><label>Pontos de limites</label><repeat nodeset="/data/Pontos"><input accuracyThreshold="1.5" ref="/data/Pontos/auto_gps_limit"><label>Gravar um ponto</label><hint>Gravar um ponto a qualquer mudança de direcção, e pelo menos uma vez a cada dez metros. Lembrete: se a 'localização record' está levando algum tempo, por favor, verifique se a luz azul do seu Glo</hint></input></repeat></group><input ref="/data/endnote"><label>&lt;h1&gt;Está quase no fim do formulário. 

&lt;h2&gt;Agora pode preencher o recibo e tirar uma foto do recibo

&lt;span style="color:#FF6347"&gt;Deve entregar o original ao requerente.&lt;/span&gt; 

&lt;span style="color:#57b055"&gt;A primeira copia do original vai para a Associação Comunitária.&lt;/span&gt; 

&lt;span style="color:#042f66"&gt;A segunda copia do original fica no livro de recibos.&lt;/span&gt;</label></input><upload mediatype="image/*" ref="/data/receipt_image"><label>Tire uma fotografia do recibo</label></upload></h:body></h:html>