<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>Form M - Cartograma da comunidade</h:title><model odk:xforms-version="1.0.0"><submission action="http://cavateco.terrafirma.co.mz:8080/wcs_nampula/submission" method="post"/><itext><translation default="true()" lang="default"><text id="/data/g1/logo:label"><value form="image">jr://images/terrafirma_wcs.jpg</value></text></translation></itext><instance><data id="form_m_cartograma" version="2022012801"><start/><deviceid/><external_id/><project/><my_form_name/><g1><logo/><cavateco/></g1><intronote/><searchtext/><enum_name_id/><enum_name/><date/><prov_id/><dist_id/><post_id/><assoc_id/><assoc_name/><cartograma_image/><obs/><end/><meta><instanceID/><instanceName/></meta></data></instance><instance id="enum_names_wcs_nampula" src="jr://file-csv/enum_names_wcs_nampula.csv"/><instance id="assoc_names_wcs_nampula" src="jr://file-csv/assoc_names_wcs_nampula.csv"/><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/><bind calculate="&quot;fixed&quot;" nodeset="/data/external_id" type="string"/><bind calculate="&quot;WCS - Futura Azul&quot;" nodeset="/data/project" type="string"/><bind calculate="&quot;Formulário M - Cartograma&quot;" nodeset="/data/my_form_name" type="string"/><bind nodeset="/data/g1/logo" readonly="true()" type="string"/><bind nodeset="/data/g1/cavateco" readonly="true()" type="string"/><bind nodeset="/data/intronote" readonly="true()" type="string"/><bind nodeset="/data/searchtext" required="true()" type="string"/><bind nodeset="/data/enum_name_id" required="true()" type="string"/><bind calculate="pulldata('enum_names_wcs_nampula', 'enumerator_name', 'name_id_key',  /data/enum_name_id )" nodeset="/data/enum_name" type="string"/><bind constraint=". &lt;= today()" nodeset="/data/date" required="true()" type="date"/><bind nodeset="/data/prov_id" required="true()" type="string"/><bind nodeset="/data/dist_id" required="true()" type="string"/><bind nodeset="/data/post_id" required="true()" type="string"/><bind nodeset="/data/assoc_id" required="true()" type="string"/><bind calculate="pulldata('assoc_names_wcs_nampula', 'assoc_name', 'assoc_id_key',  /data/assoc_id )" nodeset="/data/assoc_name" type="string"/><bind nodeset="/data/cartograma_image" required="true()" type="binary"/><bind constraint="regex(., &quot;^[a-zA-Z0-9. _-éáíóãõúçâê]{1,255}$&quot;)" nodeset="/data/obs" type="string"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/><bind calculate="concat('cartograma','_', /data/assoc_name )" nodeset="/data/meta/instanceName" type="string"/></model></h:head><h:body><group appearance="field-list" ref="/data/g1"><input ref="/data/g1/logo"><label ref="jr:itext('/data/g1/logo:label')"/></input><input ref="/data/g1/cavateco"><label>&lt;br/&gt; &lt;h1&gt;Cadeia de Valor de Terra Comunitária&lt;/h1&gt;&lt;br/&gt; &lt;h2&gt;<output value=" /data/project "/>&lt;/h2&gt;&lt;span style=&quot;color:#57b055&quot;&gt;<output value=" /data/my_form_name "/>&lt;/span&gt; &lt;br/&gt;</label></input></group><input ref="/data/intronote"><label>&lt;span style=&quot;color:#FF0000&quot;&gt;<output value=" /data/project "/>&lt;/span&gt;&lt;br/&gt; &lt;span style=&quot;color:#57b055&quot;&gt;<output value=" /data/my_form_name "/>&lt;/span&gt; &lt;br/&gt;

